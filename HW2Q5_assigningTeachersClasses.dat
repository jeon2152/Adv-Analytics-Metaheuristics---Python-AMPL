	     (tp1, *) p1 p2 p3 p4 p5 p6
	     (tp2, *) p1 p2 p3 p4 p5 p6
	     (tp3, *) p1 p2 p3 p4 p5 p6
	     (tp4, *) p1 p2 p3 p4 p5 p6

