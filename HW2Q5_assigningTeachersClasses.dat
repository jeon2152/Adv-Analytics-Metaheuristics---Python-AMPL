	     p1 p2 p3 p4 p5 p6			#period nodes
	     r1 r2				#classroom nodes
	     SRC				#source node
	     SNK;				#sink node


set ARCS  := (SRC, *) c1 c2 c3 c4 c5 c6 c7 c8

	     (c1, cp1) (c2, cp2) (c3, cp3) (c4, cp4) (c5, cp5) (c6, cp6) (c7, cp7) (c8, cp8)

